</style><script src="https://unpkg.com/@studio-freight/lenis@1.0.42/dist/lenis.min.js"></script><script>// Initialize Lenis
    const lenis = new Lenis()
    // Function to check if any modal is open
    function isAnyModalOpen() {
        const modals = document.querySelectorAll('[modal]')
        return Array.from(modals).some(modal => modal.classList.contains('w--open'))
    }
    let isScrollBlocked = false
    // Function to add window scroll listeners
    function addScrollListeners() {
        window.addEventListener('wheel', checkToUnblockScroll, { passive: true })
        window.addEventListener('touchstart', checkToUnblockScroll, { passive: true })
    }
    // Function to remove window scroll listeners
    function removeScrollListeners() {
        window.removeEventListener('wheel', checkToUnblockScroll)
        window.removeEventListener('touchstart', checkToUnblockScroll)
    }
    // Function to check if we should unblock scroll
    function checkToUnblockScroll() {
        if (isScrollBlocked && !isAnyModalOpen()) {
            lenis.start()
            isScrollBlocked = false
            removeScrollListeners()
        }
    }
    // Function to block scroll
    function blockScroll() {
        if (!isScrollBlocked) {
            lenis.stop()
            isScrollBlocked = true
            addScrollListeners()
        }
    }
    // Function to handle modal state changes
    const observeModalChanges = () => {
        const modals = document.querySelectorAll('[modal]')
        modals.forEach(modal => {
            // Create an observer instance
            const observer = new MutationObserver(mutations => {
                mutations.forEach(mutation => {
                    if (mutation.attributeName === 'class') {
                        if (modal.classList.contains('w--open')) {
                            blockScroll()
                        } else {
                            checkToUnblockScroll()
                        }
                    }
                })
            })
            // Start observing the modal for class changes
            observer.observe(modal, { attributes: true })
        })
    }
    // Lenis scroll handler
    function raf(time) {
        lenis.raf(time)
        requestAnimationFrame(raf)
    }
    // Start the animation frame loop
    requestAnimationFrame(raf)
    // Initial setup
    lenis.on('scroll', () => {
        if (isAnyModalOpen()) {
            blockScroll()
        }
    })
    // Start observing modals
    observeModalChanges()
</script><script src="{{ '_helper.js' | asset_url }}" type="text/javascript"></script>{{ content_for_header }}</head>
