{% endif %}    </div>
{% endif %}</div></div></div></div> {% schema %} {"tag":"section","name":"product_row","class":"section_feat-collect-products","settings":[{"id":"collection_collection","type":"collection","label":"Collection"},{"id":"text_heading","type":"text","label":"Heading","default":"Featured"}],"presets":[{"name":"product_row","category":"Liquify"}]} {% endschema %}